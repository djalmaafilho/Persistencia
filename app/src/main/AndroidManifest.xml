<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="dpassos.com.br.persistencia" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ListaDadosActivity"
            android:label="@string/title_activity_lista_dados" >
        </activity>
        <activity
            android:name=".PrefereciasActivity"
            android:label="@string/title_activity_preferecias" >
        </activity>

        <provider
            android:name=".PersistenciaProvider"
            android:authorities="dpassos.com.br.persistencia"
            android:enabled="true"
            android:exported="true" >
        </provider>

        <activity
            android:name=".ProviderClienteActivity"
            android:label="@string/title_activity_provider_cliente" >
        </activity>
    </application>

</manifest>
